{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@corvina/device-client/(.*)": "<rootDir>/../libs/device-client/src/$1",
    "@corvina/device-client": "<rootDir>/../libs/device-client/src"
  }
}